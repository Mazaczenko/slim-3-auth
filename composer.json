        "slim/slim": "^3.0",
		"illuminate/database": "^5.2",
		"illuminate/pagination": "^5.2",
		"slim/twig-view": "^2.0",
		"slim/flash": "^0.1.0",
		"slim/csrf": "^0.6.0",
		"ircmaxell/random-lib": "^1.1",
		"phpmailer/phpmailer": "^5.2",
		"vlucas/phpdotenv": "^3",
        "hassankhan/config": "^0.10.0",
		"symfony/console": "^3.1",
		"google/recaptcha": "~1.1",
		"robmorgan/phinx": "^0.10.7"
    }
}
